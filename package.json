{
  "name": "ngx-base",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --aot --prod",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "pretty": "prettier --parser typescript --loglevel warn --write \"src/**/*.ts\"",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postinstall:old": "ng build --aot --prod",
    "start": "node server.js",
    "start:local": "ng serve --aot --port 4300 --configuration=local",
    "start:prod": "ng serve --aot --port 4300 --configuration=production",
    "start:prod:test": "ng serve --aot --port 3002 --configuration=production --optimization=false",
    "test": "ng test --code-coverage --watch true",
    "test:once": "ng test --code-coverage --watch=false --progress=false --karma-config karma.ci.conf.js",
    "translate": "ng xi18n --output-path locale && ng xi18n --output-path locale --i18n-locale nl --out-file messages.nl.xlf"
  },
  "precommit": [
    "pretty",
    "lint",
    "postinstall"
  ],
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.7"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/localize": "^9.0.2",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.7.5"
  }
}
